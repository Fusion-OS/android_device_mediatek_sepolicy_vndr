[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group Fusion-OS
	owner = group user/Box In a Box (Box-boi)
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group Fusion-OS
	label-Code-Review = -2..+2 group user/Box In a Box (Box-boi)
	label-Verified = -1..+1 group Fusion-OS
	label-Verified = -1..+1 group user/Box In a Box (Box-boi)
	push = +force group Fusion-OS
	push = +force group user/Box In a Box (Box-boi)
	read = group Fusion-OS
	read = group user/Box In a Box (Box-boi)
	rebase = group Fusion-OS
	rebase = group user/Box In a Box (Box-boi)
	removeReviewer = group Fusion-OS
	removeReviewer = group user/Box In a Box (Box-boi)
	submit = group Fusion-OS
	submit = group user/Box In a Box (Box-boi)
	exclusiveGroupPermissions = push
[access "refs/heads/*"]
	create = group Fusion-OS
	create = group user/Box In a Box (Box-boi)
	push = +force group Fusion-OS
	push = +force group user/Box In a Box (Box-boi)
	pushMerge = group Fusion-OS
	pushMerge = group user/Box In a Box (Box-boi)
	read = group Fusion-OS
	read = group user/Box In a Box (Box-boi)
	exclusiveGroupPermissions = push
[access "refs/tags/*"]
	push = +force group Fusion-OS
	push = +force group user/Box In a Box (Box-boi)
	exclusiveGroupPermissions = push
